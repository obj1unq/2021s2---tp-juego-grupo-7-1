import wollok.game.*
import heroina.*
import config.*

program juego {
	game.title("Space Invaders")
	game.cellSize(20)
	game.width(50)
	game.height(30)
	heroina.composition().forEach({f=>
	  f.forEach({c=>
    	game.addVisual(c)	    
	  })
	})
	game.addVisual(heroina.anchor())
	config.configurarTeclas()
	game.start()
}